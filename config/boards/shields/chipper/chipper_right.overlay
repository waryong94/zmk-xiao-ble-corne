#include "chipper.dtsi"

&five_column_transform {
	col-offset = <6>;
};

&default_transform {
	col-offset = <6>;
};


&kscan0 {
	col-gpios
		= <&gpio0 4 GPIO_ACTIVE_HIGH>   //Col0
		, <&gpio0 5 GPIO_ACTIVE_HIGH>   //Col1
		, <&gpio1 11 GPIO_ACTIVE_HIGH>   //Col2
		, <&gpio0 4 GPIO_ACTIVE_HIGH>   //Col1
		, <&gpio0 5 GPIO_ACTIVE_HIGH>   //Col1
		, <&gpio1 11 GPIO_ACTIVE_HIGH>   //Col2
		;
};
